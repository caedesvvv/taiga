<Addin id="ModularRex.RexNetwork" version="0.1">
  <Runtime>
    <Import assembly="ModularRex.dll"/>
  </Runtime>

  <Dependencies>
    <Addin id="OpenSim" version="0.5" />
  </Dependencies>

  <Extension path = "/OpenSim/RegionModules">
    <RegionModule id="RexSceneUploadReceiver" type="ModularRex.RexNetwork.SceneUploadReceiver" />
    <RegionModule id="RexLoginModule" type="ModularRex.RexNetwork.RexLogin.RexLoginModule" />
    <RegionModule id="RexLoginConfigurationManager" type="ModularRex.RexNetwork.RexLogin.LoginConfigurationManager" />
  </Extension>
</Addin>
